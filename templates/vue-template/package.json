    "object-to-formdata": "^4.5.1",
    "pinia": "^2.2.6",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "vue-bitrix24": "^6.0.0",
    "vue-router": "^4.4.5",
    "vue-select": "^4.0.0-beta.6"
  },
