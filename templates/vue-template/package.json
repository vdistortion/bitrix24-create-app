{
  "name": "bitrix24-create-app",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 4200",
    "watch": "vite build --watch",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build && tsx archiving.ts",
    "preview": "vite preview --host",
    "type-check": "vue-tsc --build",
    "format": "prettier . --write",
    "grid": "tsx grid.ts && npx sass-migrator division src/assets/_grid.scss && prettier --write src/assets/_grid.scss",
    "grep": "sh grep.sh"
  },
  "dependencies": {
    "@jamescoyle/vue-icon": "^0.1.2",
    "@mdi/js": "^7.4.47",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^11.0.2",
    "axios": "^1.9.0",
    "bitrix24-library": "^1.0.0",
    "object-to-formdata": "^4.5.1",
    "pinia": "^3.0.1",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-bitrix24": "^6.0.1",
    "vue-router": "^4.5.0",
    "vue-select": "^4.0.0-beta.6"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/archiver": "^6.0.3",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "archiver": "^7.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.87.0",
    "smart-grid": "^2.1.2",
    "tsx": "^4.19.4",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
